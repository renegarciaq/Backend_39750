<div>
   {{#if users.length }}
   <table id="tableUsers" class="table table-striped table-dark">
      <thead>
         <tr>
            <th scope="col">name</th>
            <th scope="col">lastName</th>
            <th scope="col">age</th>
            <th scope="col">address</th>
            <th scope="col">email</th>
            <th scope="col">genero</th>
            <th scope="col">password</th>
         </tr>
      </thead>
      {{#each users}}
      <tbody>
         <tr>
            <td>{{this.first_name}}</td>
            <td>{{this.lastName}}</td>
            <td>{{this.age}}</td>
            <td>{{this.address}}</td>
            <td>{{this.email}}</td>
            <td>{{this.genero}}</td>
            <td>{{this.password}}</td>
         </tr>
      </tbody>
      {{/each}}
   </table>
   {{else}}
   <h3>No hay usuarios agregados!</h3>
   {{/if}}
</div>
<div class="row d-flex justify-content-around">
   <div class="col-md-3 d-inline-flex p-2">
      <form id="userDelete" class="card card-body rounded-0 text-center">
         <h3 class="mb-5">Delete a user</h3>
         <input type="number" required id="deleteUser" name="deleteUser" placeholder="Ingrese el id"
            class="form-control rounded-0 mb-3">
         <input class="btn btn-dark rounded-0 mb-3" type="submit" value="Eliminar">
      </form>
   </div>
   <div class="col-md-7 d-inline-flex p-2">
      <form id="addUser" class="card card-body rounded-0 text-center" action="">
         <h3 class="mb-5">Add a User</h3>
         <input type="text" required id="first_name" name="first_name" placeholder="Ingrese el nombre"
            class="form-control rounded-0 mb-3">
         <input type="text" required id="lastName" name="lastName" placeholder="Ingrese el apellido"
            class="form-control rounded-0 mb-3">
         <input type="number" required id="age" name="age" placeholder="Ingrese la edad"
            class="form-control rounded-0 mb-3">
         <input type="text" required id="address" name="address" placeholder="Ingrese el domicio"
            class="form-control rounded-0 mb-3">
         <input type="text" required id="email" name="email" placeholder="Ingrese email"
            class="form-control rounded-0 mb-3">
         <input type="text" required id="genero" name="genero" placeholder="Ingrese genero"
            class="form-control rounded-0 mb-3">
         <input type="password" required id="password" name="password" placeholder="Ingrese password"
            class="form-control rounded-0 mb-3">
         <input class="btn btn-dark rounded-0 mb-3" type="submit" value="Cargar">
      </form>
   </div>
</div>
<script src="/socket.io/socket.io.min.js"></script>
<script src="static/js/users.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.7.1/dist/sweetalert2.all.min.js "></script>